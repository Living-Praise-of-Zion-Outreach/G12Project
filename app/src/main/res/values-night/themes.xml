<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.G12Project" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/primary_light</item>
        <item name="colorPrimaryVariant">@color/primary_dark</item>
        <item name="colorOnPrimary">@color/_super_dark_fifth</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/night_button</item>
        <item name="colorSecondaryVariant">@color/night_button</item>
        <item name="colorOnSecondary">@color/white</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l" tools:ignore="ObsoleteSdkInt">@color/_super_dark_fifth</item>

        <!-- Customize your theme here. -->
        <!-- THIS IS FOR CHANGING ICON COLOR IN TOOLBAR -->
        <item name="android:textColorSecondary">@color/white</item>
        <!-- THIS IS FOR CHANGING TEXT COLOR IN TOOLBAR -->
        <item name="android:textColorPrimary">@color/white</item>
        <!-- THIS IS FOR CHANGING TEXT COLOR SUB ITEM IN TOOLBAR -->
        <item name="android:itemTextAppearance">@style/menuItem</item>
        <!-- THIS IS FOR CHANGING ICON COLOR DRAWER TOOLBAR -->
        <item name="drawerArrowStyle">@style/DrawerIconStyle</item>

        <item name="android:navigationBarColor">@color/_super_dark_fifth</item>

        <!-- THIS IS ACTION MODE BEHAVIOR TOOLBAR -->
        <item name="android:actionModeBackground">@color/_super_dark_fifth</item>
        <item name="android:actionModeSplitBackground">@color/accent</item>
        <item name="android:windowActionModeOverlay">true</item>
        <item name="windowActionModeOverlay">true</item>

        <item name="colorAccent">@color/_seven</item>
        <item name="colorControlActivated">@color/_seven</item>
    </style>

    <style name="ThemeOverlay.PopupTheme" parent="Theme.AppCompat.Light.DarkActionBar">
