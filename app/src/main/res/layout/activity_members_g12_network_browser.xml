<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    style="@style/background_group_box_light"
    tools:context=".Activity.MembersG12NetworkBrowser">
    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar_g12_network_browser"
        style="@style/custom_ToolBar"
        />

    <RelativeLayout
        style="@style/background_group_box_light"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="top"
        android:layout_marginTop="?attr/actionBarSize"
        android:focusableInTouchMode="true"
        android:gravity="top"
        android:screenOrientation="portrait">

        <LinearLayout
            style="@style/background_group_box_light"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentBottom="true"
            android:orientation="vertical"
            android:padding="0dp">

            <androidx.constraintlayout.widget.ConstraintLayout
                style="@style/background_group_box_light"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <androidx.cardview.widget.CardView
                    android:id="@+id/cv_network_leader_logo_network"
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginStart="10dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:cardCornerRadius="50dp"
                    >
                    <ImageView
                        android:id="@+id/iv_name_logo_network"
                        android:layout_width="80dp"
                        android:layout_height="80dp"
                        android:scaleType="fitXY"
                        android:src="@drawable/default_photo"
                        />


                </androidx.cardview.widget.CardView>

                <TextView
                    android:id="@+id/tv_organization_name_network"
                    style="@style/label_header"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Network Leader"
                    android:textSize="11sp"
                    android:layout_marginStart="10dp"
                    android:textStyle="italic"

                    app:layout_constraintStart_toEndOf="@+id/cv_network_leader_logo_network"
                    app:layout_constraintTop_toBottomOf="@+id/tv_network_leader_network_browse"
                    tools:ignore="SmallSp" />

                <TextView
                    android:id="@+id/textView4"
                    style="@style/label_header"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Name"
                    android:layout_marginStart="10dp"
                    android:textSize="11sp"
                    android:textStyle="italic"
                    app:layout_constraintStart_toEndOf="@+id/cv_network_leader_logo_network"
                    app:layout_constraintTop_toBottomOf="@+id/tv_name_network_browse" />

                <TextView
                    android:id="@+id/tv_name_network_browse"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="10dp"
                    android:layout_marginTop="10dp"
                    android:ellipsize="end"
                    android:maxLines="1"
                    android:text="SYLVSTER R. BELONIO"
                    android:textSize="11sp"
                    android:textStyle="bold"
                    app:layout_constraintEnd_toStartOf="@+id/constraintLayout5"
                    app:layout_constraintStart_toEndOf="@+id/cv_network_leader_logo_network"
                    app:layout_constraintTop_toTopOf="parent" />


                <TextView
                    android:id="@+id/tv_network_leader_network_browse"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="10dp"
                    android:ellipsize="end"
                    android:maxLines="1"
                    android:text="ANNIE QUERUBIN"
                    android:textSize="11sp"
                    android:textStyle="bold"
                    app:layout_constraintEnd_toStartOf="@+id/constraintLayout5"
                    app:layout_constraintStart_toEndOf="@+id/cv_network_leader_logo_network"
                    app:layout_constraintTop_toBottomOf="@+id/textView4" />

                <TextView
                    android:id="@+id/tv_organization_name_network2"
                    style="@style/label_header"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Organization"
                    android:textSize="11sp"
                    android:textStyle="italic"

                    app:layout_constraintStart_toStartOf="@+id/tv_network_leader_network_browse"
                    app:layout_constraintTop_toBottomOf="@+id/tv_network_name_browse"
                    tools:ignore="SmallSp" />


                <TextView
                    android:id="@+id/tv_network_name_browse"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:ellipsize="end"
                    android:maxLines="1"
                    android:layout_marginStart="10dp"
                    android:text="ANNIE QUERUBIN"
                    android:textSize="11sp"
                    android:textStyle="bold"
                    app:layout_constraintStart_toEndOf="@+id/cv_network_leader_logo_network"
                    app:layout_constraintTop_toBottomOf="@+id/tv_organization_name_network" />
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/constraintLayout5"
                    android:layout_width="wrap_content"

                    android:layout_height="wrap_content"
                    android:paddingStart="10dp"
                    android:paddingEnd="10dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toStartOf="@+id/constraintLayout4"
                    app:layout_constraintTop_toTopOf="parent">

                    <ImageView
                        android:id="@+id/imageView7"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:src="@drawable/ic_icon_person"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:tint="@color/_fourth" />

                    <TextView
                        android:id="@+id/tv_current_members_leaders_network_browse"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="0"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/imageView7" />

                </androidx.constraintlayout.widget.ConstraintLayout>


                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/constraintLayout4"
                    android:layout_width="wrap_content"

                    android:layout_height="wrap_content"
                    android:paddingStart="10dp"
                    android:paddingEnd="10dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toStartOf="@+id/constraintLayout3"

                    app:layout_constraintTop_toTopOf="parent">

                    <ImageView
                        android:id="@+id/imageView6"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:src="@drawable/ic_icon_people"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:tint="@color/_third" />

                    <TextView
                        android:id="@+id/tv_current_144_members_network_browse"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"

                        android:text="0"

                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/imageView6" />

                </androidx.constraintlayout.widget.ConstraintLayout>

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/constraintLayout3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="10dp"
                    android:paddingStart="10dp"
                    android:paddingEnd="10dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <ImageView
                        android:id="@+id/imageView5"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:src="@drawable/ic_icon_network"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:tint="@color/warning" />

                    <TextView
                        android:id="@+id/tv_current_1789_members_network_browse"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="0"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/imageView5" />
                </androidx.constraintlayout.widget.ConstraintLayout>


            </androidx.constraintlayout.widget.ConstraintLayout>

            <com.google.android.material.tabs.TabLayout
                android:id="@+id/tl_network_browse"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:layout_marginTop="10dp"
                style="@style/TabLayoutPower"
                />
            <androidx.viewpager2.widget.ViewPager2
                android:id="@+id/vp_network_browse"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
            </androidx.viewpager2.widget.ViewPager2>
        </LinearLayout>
    </RelativeLayout>
</RelativeLayout>