<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.G12Project" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/primary</item>
        <item name="colorPrimaryVariant">@color/_dark_first</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/primary</item>
        <item name="colorSecondaryVariant">@color/_fourth</item>
        <item name="colorOnSecondary">@color/white</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:ignore="ObsoleteSdkInt" tools:targetApi="l">
            @color/_dark_first
        </item>

        <!-- Customize your theme here. -->
        <!-- THIS IS FOR CHANGING ICON COLOR IN TOOLBAR -->
        <item name="android:textColorSecondary">@color/white</item>
        <!-- THIS IS FOR CHANGING TEXT COLOR IN TOOLBAR -->
        <item name="android:textColorPrimary">@color/white</item>
        <!-- THIS IS FOR CHANGING TEXT COLOR SUB ITEM IN TOOLBAR -->
        <item name="android:itemTextAppearance">@style/menuItem</item>
        <!-- THIS IS FOR CHANGING ICON COLOR DRAWER TOOLBAR -->
        <item name="drawerArrowStyle">@style/DrawerIconStyle</item>

        <!-- THIS IS ACTION MODE BEHAVIOR TOOLBAR -->
        <item name="android:actionModeBackground">@color/primary</item>
        <item name="android:actionModeSplitBackground">@color/black</item>
        <item name="android:windowActionModeOverlay">true</item>
        <item name="windowActionModeOverlay">true</item>

    </style>

    <style name="ThemeOverlay.PopupTheme" parent="Theme.AppCompat.Light.DarkActionBar">
    <!-- Arrow Icon Sub Menu Color -->
    <item name="android:textColorSecondary">@color/primary</item>
    <!-- Primary brand color. -->
    <!--  <item name="android:colorBackground">@color/light_gray</item> -->
    <!-- Color of Sub Header Text -->
    <item name="android:textColor">@color/primary</item>
    </style>

    <!-- SPINNER SECTION -->
    <style name="spinner_style">
        <item name="android:paddingLeft">0dp</item>
        <item name="android:dropDownWidth">533dp</item>
        <item name="android:showDividers">none</item>
        <item name="android:dividerHeight">0dp</item>
        <item name="android:popupBackground">@drawable/bg_round</item>
        <item name="android:dropDownHeight">70dp</item>
        <item name="android:scrollbarAlwaysDrawVerticalTrack">true</item>
        <item name="android:dropDownSelector">@android:color/white</item>
    </style>

    <style name="FloatActionButton_Profile">
        <item name="android:backgroundTint">@color/accent</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginEnd">24dp</item>
        <item name="android:layout_marginBottom">24dp</item>
        <item name="android:clickable">true</item>
        <item name="android:layout_gravity">end|bottom</item>
        <item name="android:focusable">true</item>
    </style>

    <style name="CardView_Primary_Photo_Profile">
        <item name="android:layout_width">150dp</item>
        <item name="android:layout_height">150dp</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_marginBottom">16dp</item>
        <item name="cardCornerRadius">80dp</item>
        <item name="cardElevation">10dp</item>
    </style>

